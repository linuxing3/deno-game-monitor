{
  "cmds": {
    "multi": [
      "ls",
      "pwd"
    ],
    "install": "deno install -f --allow-run --allow-read https://raw.githubusercontent.com/apiel/den/master/den.ts",
    "ls": "ls -l",
    "start": "deno run -A --importmap=import_map.json --unstable -c tsconfig.json server.ts",
    "pm": "nohup deno run -A --unstable -c tsconfig.json server.ts &",
    "start:slim": "deno run -A --unstable -c tsconfig.json server.slim.ts",
    "test": "deno test -A --importmap=import_map.json --unstable",
    "gen:generator": "hygen generator with-prompt",
    "gen:model": "hygen model new",
    "gen:route": "hygen route new",
    "gen:controller": "hygen controllers new",
    "gen:service": "hygen services new",
    "gen:test": "hygen tests new",
    "gen:mid": "hygen middleware new",
    "cache:deps": "deno cache --unstable deps.ts",
    "fmt": "deno fmt",
    "deploy": [
      "deno fmt",
      "git add .",
      "git commit -m den",
      "git push"
    ],
    "lint": "deno lint --unstable",
    "note": "deno run -A --unstable tests/note/note_app.ts",
    "note:l": "deno run -A --unstable .lizard_installer.ts",
    "logger:log": "deno run -A --unstable tests/logger/smart-logger.ts log  -f csv /tmp/games",
    "logger:filter": "deno run -A --unstable tests/logger/smart-logger.ts filter -f csv -k Code /tmp/games",
    "logger:json": "deno run -A --unstable tests/logger/smart-logger.ts json -f csv -k Code.exe /tmp/games",
    "logger:pids": "deno run -A --unstable tests/logger/smart-logger.ts pids -f csv -k Code.exe /tmp/games",
    "logger:send": "deno run -A --unstable tests/logger/smart-logger.ts send -f csv -k Code.exe /tmp/games",
    "logger:kill": "deno run -A --unstable tests/logger/smart-logger.ts kill -f csv -k Code.exe /tmp/games"
  }
}